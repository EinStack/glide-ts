	"author": "EinStack <contact@einstack.ai>",
	"keywords": ["llm", "ai", "gateway"],
	"description": "A minimal Glide client",
	"repository": "git+https://github.com/EinStack/glide-ts.git",
	"bugs": "https://github.com/EinStack/glide-ts/issues",
